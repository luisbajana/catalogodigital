<h2>Sign up</h2>

<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <div><%= f.label :nombre %><br />
  <%= f.text_field :nombre, :autofocus => true %></div>

  <div><%= f.label :email %><br />
  <%= f.email_field :email %></div>

  <div><%= f.label :password %><br />
  <%= f.password_field :password %></div>

  <div><%= f.label :password_confirmation %><br />
  <%= f.password_field :password_confirmation %></div>

  <div><%= f.label :telefono %><br />
  <%= f.text_field :telefono %></div>

  <div><%= f.label :industria %><br />
  <%= f.text_field :industria %></div>

  <div><%= f.label :pais %><br />
  <%= f.text_field :pais %></div>

  <div><%= f.label :codigo_especial %><br />
  <%= f.text_field :codigo_especial %></div>

  <div><%= f.submit "Registrarse" %></div>
<% end %>

<%= render "devise/shared/links" %>
